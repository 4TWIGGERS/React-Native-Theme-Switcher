{
 "expo": {
  "name": "React-Native-Theme-Switcher",
  "slug": "themeSwitcher",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",
  "splash": {
   "image": "./assets/splash.png",
   "resizeMode": "contain",
   "backgroundColor": "#ffffff"
  },
  "assetBundlePatterns": ["**/*"],
  "ios": {
   "supportsTablet": true,
   "bundleIdentifier": "com.4twiggers.theme-switcher"
  },
  "android": {
   "adaptiveIcon": {
    "foregroundImage": "./assets/adaptive-icon.png",
    "backgroundColor": "#ffffff"
   },
   "package": "test.twiggers.com"
  }
 }
}
